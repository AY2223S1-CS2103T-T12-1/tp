@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<interface>>\nReadOnlyTaAssist" as ReadOnlyTaAssist
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class TaAssist
Class ModelManager
Class UserPrefs

Class UniqueStudentList
Class UniqueModuleClassList
Class Student
Class Address
Class Email
Class Name
Class Phone
Class ModuleClass
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

TaAssist .up.|> ReadOnlyTaAssist

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyTaAssist
ModelManager -left-> "1" TaAssist
ModelManager -right-> "1" UserPrefs
ModelManager ---> "0..1 \nfocused" ModuleClass
UserPrefs .up.|> ReadOnlyUserPrefs

TaAssist *---> "1" UniqueStudentList
TaAssist *--> "1" UniqueModuleClassList

UniqueStudentList -right-> "~* all" Student
UniqueModuleClassList -left-> "all*" ModuleClass
Student *--> "1" Name
Student *--> "0..1" Phone
Student *--> "0..1" Email
Student *--> "0..1" Address
Student -up-> "~*" ModuleClass

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager -left->"~* filtered" Student
@enduml
